<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/diagnostics"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:AndroidProject"
             mc:Ignorable="d"
             x:Class="AndroidProject.MainPage">
    <StackLayout Padding="20">
        <Label Text="Enter start number:" />
        <Entry x:Name="StartNumberEntry" Keyboard="Numeric" />

       
        <Label Text="Enter end number:" />
        <Entry x:Name="EndNumberEntry" Keyboard="Numeric" />

        <Label Text="Enter increment value:" />
        <Entry x:Name="IncrementEntry" Keyboard="Numeric" />

        <Button Text="Calculate" Clicked="CalculateButton_Clicked" />

        <Label x:Name="ResultLabel" FontSize="18" FontAttributes="Bold" />
    </StackLayout>
</ContentPage>
